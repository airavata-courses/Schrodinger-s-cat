pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {

                script{
                withEnv(['JENKINS_NODE_COOKIE=dontKillMe']) {
                    sh "nohup docker-compose up > pythonServerLog.log 2>&1 &"
                }
            }
            }
        }
    }
}
